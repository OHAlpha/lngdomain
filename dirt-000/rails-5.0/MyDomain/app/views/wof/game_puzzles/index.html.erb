<p id="notice"><%= notice %></p>

<h1>Wof Game Puzzles</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Puzzle</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wof_game_puzzles.each do |wof_game_puzzle| %>
      <tr>
        <td><%= wof_game_puzzle.game %></td>
        <td><%= wof_game_puzzle.puzzle %></td>
        <td><%= wof_game_puzzle.role %></td>
        <td><%= link_to 'Show', wof_game_puzzle %></td>
        <td><%= link_to 'Edit', edit_wof_game_puzzle_path(wof_game_puzzle) %></td>
        <td><%= link_to 'Destroy', wof_game_puzzle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wof Game Puzzle', new_wof_game_puzzle_path %>
