<p id="notice"><%= notice %></p>

<h1>Wof Games</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sequence</th>
      <th>State</th>
      <th>Status</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wof_games.each do |wof_game| %>
      <tr>
        <td><%= wof_game.name %></td>
        <td><%= wof_game.sequence %></td>
        <td><%= wof_game.state %></td>
        <td><%= wof_game.status %></td>
        <td><%= wof_game.details %></td>
        <td><%= link_to 'Show', wof_game %></td>
        <td><%= link_to 'Edit', edit_wof_game_path(wof_game) %></td>
        <td><%= link_to 'Destroy', wof_game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wof Game', new_wof_game_path %>
