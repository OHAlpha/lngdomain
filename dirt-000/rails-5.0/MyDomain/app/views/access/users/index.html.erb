<p id="notice"><%= notice %></p>

<h1>Access Users</h1>

<table>
  <thead>
    <tr>
      <th>Unom</th>
      <th>Email</th>
      <th>Salt</th>
      <th>Hashkey</th>
      <th>Iterations</th>
      <th>Passhash</th>
      <th>Fnom</th>
      <th>Mnoms</th>
      <th>Lnom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_users.each do |access_user| %>
      <tr>
        <td><%= access_user.unom %></td>
        <td><%= access_user.email %></td>
        <td><%= access_user.salt %></td>
        <td><%= access_user.hashkey %></td>
        <td><%= access_user.iterations %></td>
        <td><%= access_user.passhash %></td>
        <td><%= access_user.fnom %></td>
        <td><%= access_user.mnoms %></td>
        <td><%= access_user.lnom %></td>
        <td><%= link_to 'Show', access_user %></td>
        <td><%= link_to 'Edit', edit_access_user_path(access_user) %></td>
        <td><%= link_to 'Destroy', access_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access User', new_access_user_path %>
